<div class="form-wrapper">
<p> Start a new match </p>
  <%= form_for [player, match] do |f| %>
  	<div class="form-label"><%= f.label :course %>
  	<%= f.collection_select(:course_id, @courses, :id, :name, {:prompt=>true}, {:class=>"form-field"}) %>

  	<div class="form-label"><%= f.label :tee %>
  	<%= f.collection_select(:tee_id, @tees, :id, :colour, {:prompt=>true}, {:class=>"form-field"}) %> 

  	<div class="form-date"><%= f.label :end_date, "Expiration Date: Two Weeks Max" %>
  	<%= f.date_field :end_date, max: 14.days.from_now, min: Date.today %>	

  	<%= f.hidden_field :player_id, :value => player.id %>

  	<%= f.submit %> 
  <% end %>

</div>